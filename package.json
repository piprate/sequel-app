{
  "name": "sequel-app",
  "description": "A web app for Sequel",
  "version": "0.13.0",
  "type": "module",
  "scripts": {
    "sample-build": "cross-env NODE_ENV=production webpack",
    "sample-dev": "webpack serve --content-base public",
    "lint": "standard && standard --plugin html 'src/routes/**/*.svelte'",
    "lint-fix": "standard --fix && standard --fix --plugin html 'src/routes/**/*.svelte'",
    "eslint": "npx eslint src/routes",
    "eslint-fix": "npx eslint src/routes --fix",
    "dev": "run-s build-template-html build-assets serve-dev",
    "serve-dev": "run-p --race build-template-html-watch vite-dev",
    "vite-dev": "vite dev --port=3005",
    "before-build": "run-s build-template-html build-assets",
    "build": "cross-env NODE_ENV=production run-s build-steps",
    "build-steps": "run-s before-build vite-export build-vercel-json",
    "vite-build": "vite build",
    "vite-preview": "vite preview",
    "start": "cross-env PORT=3005 node dist/index.js",
    "build-template-html": "node ./bin/build-template-html.js",
    "build-template-html-watch": "node ./bin/build-template-html.js --watch",
    "build-assets": "node ./bin/build-assets.js",
    "vite-export": "cross-env PORT=22939 vite build",
    "print-export-info": "node ./bin/print-export-info.js",
    "export-steps": "run-s before-build vite-export print-export-info",
    "export": "cross-env NODE_ENV=production run-s export-steps",
    "now-build": "run-s export",
    "build-vercel-json": "node ./bin/build-vercel-json.js",
    "test-dev": "vitest",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.20.6",
    "@formatjs/intl-listformat": "^7.1.7",
    "@formatjs/intl-locale": "^3.0.11",
    "@formatjs/intl-pluralrules": "^5.1.8",
    "@formatjs/intl-relativetimeformat": "^11.1.8",
    "@onflow/fcl": "^1.3.2",
    "@onflow/transport-grpc": "^1.1.4",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.5",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-url": "^8.0.1",
    "@testing-library/svelte": "^3.2.2",
    "arrow-key-navigation": "^1.2.0",
    "blurhash": "^2.0.4",
    "cheerio": "1.0.0-rc.12",
    "chokidar": "^3.5.3",
    "circular-dependency-plugin": "^5.2.2",
    "cross-env": "^7.0.3",
    "css-dedoupe": "^0.1.1",
    "email-validator": "^2.0.4",
    "emoji-picker-element": "^1.14.0",
    "emoji-picker-element-data": "^1.3.0",
    "emoji-regex": "^10.2.1",
    "escape-html": "^1.0.3",
    "eslint-plugin-html": "^7.1.0",
    "events-light": "^1.0.5",
    "fast-sha256": "^1.3.0",
    "file-drop-element": "^1.0.1",
    "file-loader": "^6.2.0",
    "focus-visible": "^5.2.0",
    "format-message-interpret": "^6.2.4",
    "format-message-parse": "^6.2.4",
    "is-emoji-supported": "^0.0.5",
    "li": "^1.3.0",
    "localstorage-memory": "^1.0.3",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "page-lifecycle": "^0.1.2",
    "pinch-zoom-element": "^1.1.1",
    "promise-worker": "^2.0.1",
    "requestidlecallback": "^0.3.0",
    "rollup": "^3.7.4",
    "rollup-plugin-svelte": "^7.1.0",
    "rtl-detect": "^1.0.4",
    "safari-14-idb-fix": "^3.0.0",
    "sass": "^1.56.2",
    "stringz": "^2.1.0",
    "svelte-i18n": "^3.6.0",
    "svelte-intl-precompile": "^0.12.0",
    "svelte-loader": "^3.1.4",
    "svgo": "^3.0.2",
    "tesseract.js": "^4.0.1",
    "tesseract.js-core": "^4.0.1",
    "text-encoding": "^0.7.0",
    "tiny-queue": "^0.2.1",
    "worker-loader": "^3.0.8"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.3",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/adapter-node": "^1.0.0",
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/adapter-vercel": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@vite-pwa/sveltekit": "^0.1.1",
    "@vitest/coverage-c8": "^0.27.0",
    "@vitest/coverage-istanbul": "^0.27.0",
    "@wwa/rollup-plugin-terser": "^1.0.1",
    "eslint": "^8.31.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^21.0.0",
    "standard": "^16.0.3",
    "stream-browserify": "^3.0.0",
    "svelte": "^3.55.0",
    "vite": "^4.0.1",
    "vite-plugin-node-polyfills": "^0.7.0",
    "vitest": "^0.27.0"
  },
  "standard": {
    "globals": [
      "AbortController",
      "Blob",
      "CSS",
      "DOMParser",
      "Element",
      "Element",
      "Event",
      "FormData",
      "HTMLElement",
      "IDBKeyRange",
      "IDBObjectStore",
      "Image",
      "ImageData",
      "IntersectionObserver",
      "MessageChannel",
      "NodeList",
      "NotificationEvent",
      "OffscreenCanvas",
      "PointerEvent",
      "Response",
      "URL",
      "WebSocket",
      "__assets__",
      "__routes__",
      "__shell__",
      "atob",
      "btoa",
      "caches",
      "customElements",
      "fetch",
      "fixture",
      "getComputedStyle",
      "history",
      "indexedDB",
      "localStorage",
      "location",
      "matchMedia",
      "performance",
      "postMessage",
      "queueMicrotask",
      "requestAnimationFrame",
      "requestIdleCallback",
      "self",
      "test"
    ]
  }
}
